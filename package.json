{
  "name": "@ocean/common",
  "version": "0.0.1",
  "description": "@ocean/common",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "tsc": "tsup --config=tsup.config.miniprogram.ts",
